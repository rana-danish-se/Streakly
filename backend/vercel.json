{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/journey-check",
      "schedule": "*/15 * * * *"
    },
    {
      "path": "/api/cron/streak-reminder",
      "schedule": "0 22 * * *"
    }
  ]
}
